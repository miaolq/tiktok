{
  "name": "TikTok Auto Next",
  "description": "Auto play next video on tiktok",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "service-worker.js"
  },
  "permissions": ["storage"],
  "action": {
    "default_icon": {
      "16": "/images/off16.png",
      "32": "/images/off32.png",
      "48": "/images/off48.png",
      "128": "/images/off128.png"
    }
  },
  "icons": {
    "16": "/images/off16.png",
    "32": "/images/off32.png",
    "48": "/images/off48.png",
    "128": "/images/off128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://www.tiktok.com/*"],
      "js": ["content-script.js"]
    }
  ]
}
