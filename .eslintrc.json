{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "arrow-parens": "off",
    "semi": "off",
    "comma-dangle": "off",
    "no-underscore-dangle": "off"
  },
  "globals": {
    "chrome": "readonly"
  }
}
